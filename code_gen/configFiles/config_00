#periodic tasks, some are critical, all independent (no dataflow between tasks)


#Comments start with #
# -c for critical task
# -T for task period in ms
# -E for event driven, runs when all the inputs are available
<FUNCTIONLIST>
-rootdir /home/jonah/fingerprinting/automotive_control/CompiledC
-name AirbagModel          -subdir AirbagModel_ert_rtw         -T  50 -c -Priority 1
-name CollisionAvoidance   -subdir CollisionAvoidance_ert_rtw  -T 100    -Priority 2
-name CruiseControlSystem  -subdir CruiseControlSystem_ert_rtw -T  20 -c -Priority 3
-name Derivative           -subdir Derivative_ert_rtw          -T  12 -c -Priority 4
-name FuelSensor           -subdir FuelSensor_ert_rtw          -T  75    -Priority 5
-name TractionControl      -subdir TractionControl_ert_rtw     -T  30 -c -Priority 6
-name TransmissionControl  -subdir TransmissionControl_ert_rtw -T  15    -Priority 7
</FUNCTIONLIST>

<DATAFLOW>
</DATAFLOW>


<PLATFORM>
-usedefault
</PLATFORM>

<MAPPING>
AirbagModel cpu0 cpu1
CollisionAvoidance cpu0
CruiseControlSystem cpum
Derivative cpu0 cpu1
FuelSensor cpu1
TractionControl cpum
TransmissionControl cpu0
</MAPPING>